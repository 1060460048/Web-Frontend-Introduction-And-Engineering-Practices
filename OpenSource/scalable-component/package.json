    "clean": "rimraf dist",
    "lint": "eslint src",
    "mocha": "./node_modules/mocha/bin/mocha -t 10000 --compilers js:babel-core/register",
    "mocha-all": "./node_modules/mocha/bin/mocha --compilers js:babel-core/register --recursive src/**/*.test.js",
    "update": "npm-check -u"
  },
  "repository": {
    "type": "git",
