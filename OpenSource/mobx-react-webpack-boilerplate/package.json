{
  "name": "mobx-react-webpack-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate For React With Mobx as StateManagement And Webpack as Bundler",
  "main": "index.js",
  "scripts": {
    "start": "node --harmony ./dev-config/devServer.js --host 0.0.0.0",
    "storybook": "start-storybook -p 9001",
    "build": "npm run clean && set NODE_ENV=production webpack -p --config ./dev-config/webpack.config.js",
    "build:library": "set NODE_ENV=library && npm run clean && webpack -p --config ./dev-config/webpack.config.js",
    "build:server": "set NODE_ENV=server && npm run clean &&  webpack -p --config ./dev-config/webpack.config.server.js",
    "deploy": "npm run build && ./node_modules/.bin/http-server dist",
    "clean": "rimraf dist",
    "lint": "eslint src",
    "mocha": "./node_modules/mocha/bin/mocha -t 10000 --compilers js:babel-core/register",
    "mocha-all": "./node_modules/mocha/bin/mocha --compilers js:babel-core/register --recursive src/**/*.test.js",
    "update": "npm-check -u"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wxyyxc1992/Web-Frontend-Introduction-And-Best-Practices/blob/master/OpenSource/mobx-react-webpack-boilerplate/README.md"
  },
  "keywords": [
    "React",
    "Mobx",
    "Webpack",
    "Boilerplate",
    "StateManagement"
  ],
  "author": "wxyyxc1992",
  "license": "ISC",
  "babel": {
    "extends": "./dev-config/.babelrc"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.14.0",
    "autoprefixer": "^6.4.1",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-3": "^6.17.0",
    "bundle-loader": "^0.5.4",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.25.0",
    "eslint": "^3.5.0",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-react": "^6.2.2",
    "eventsource-polyfill": "^0.9.6",
    "exports-loader": "^0.6.2",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "http-server": "^0.9.0",
    "image-webpack-loader": "^2.0.0",
    "imagemin": "^5.2.2",
    "imports-loader": "^0.6.5",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.1.1",
    "mocha": "^3.0.2",
    "nock": "^8.0.0",
    "node-sass": "^3.10.0",
    "object-assign": "^4.0.1",
    "postcss-flexibility": "^1.1.0",
    "postcss-loader": "^0.13.0",
    "react-komposer": "^1.13.1",
    "react-mixin": "^3.0.5",
    "rimraf": "^2.5.3",
    "sass-loader": "^4.0.2",
    "scss-loader": "^0.0.1",
    "stack-source-map": "^1.0.5",
