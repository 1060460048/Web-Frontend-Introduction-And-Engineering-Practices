    "font-awesome": "^4.6.3",
    "install": "^0.8.1",
    "isomorphic-fetch": "^2.2.1",
    "istanbul": "^0.4.5",
    "lodash": "^4.15.0",
    "normalize.css": "^4.2.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-fontawesome": "^1.2.0",
    "react-komposer": "^1.11.0",
    "react-redux": "^4.4.5",
